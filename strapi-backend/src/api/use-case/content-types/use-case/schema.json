{
  "kind": "collectionType",
  "collectionName": "use_cases",
  "info": {
    "singularName": "use-case",
    "pluralName": "use-cases",
    "displayName": "Use-Case",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titel": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "pictures": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "Titel",
      "required": true
    },
    "Images": {
      "type": "component",
      "repeatable": true,
      "component": "firmware.test"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "summary": {
      "type": "text"
    },
    "setupDuration": {
      "type": "integer",
      "default": 30,
      "min": 0,
      "required": true
    },
    "complexity": {
      "type": "integer",
      "required": true,
      "default": 1,
      "min": 1,
      "max": 5
    },
    "instructions": {
      "displayName": "instructions",
      "type": "component",
      "repeatable": true,
      "component": "instructions.instructions",
      "required": true
    },
    "costs": {
      "type": "decimal",
      "min": 0,
      "default": 0
    },
    "firms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::firm.firm"
    },
    "components": {
      "type": "dynamiczone",
      "components": [
        "thingsboard.asset-profile",
        "thingsboard.component",
        "thingsboard.dashboard",
        "thingsboard.rule-chain"
      ]
    },
    "setupSteps": {
      "type": "dynamiczone",
      "components": [
        "instructions.setup-instruction",
        "instructions.list-instruction",
        "instructions.text-instruction"
      ],
      "required": true,
      "min": 1
    }
  }
}
