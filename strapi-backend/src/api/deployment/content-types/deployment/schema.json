{
  "kind": "collectionType",
  "collectionName": "deployments",
  "info": {
    "singularName": "deployment",
    "pluralName": "deployments",
    "displayName": "Setups",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-type-builder": {
      "visible": true
    }
  },
  "attributes": {
    "use_case": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::use-case.use-case",
      "configurable": false
    },
    "firm": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::firm.firm",
      "configurable": false
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "created",
        "deploying",
        "deployed"
      ],
      "default": "created",
      "required": true,
      "configurable": false
    },
    "sync": {
      "type": "boolean",
      "default": false,
      "required": true,
      "configurable": false
    },
    "CustomerUID": {
      "type": "customField",
      "customField": "plugin::thingsboard-plugin.thingsboardUserId",
      "unique": true,
      "configurable": false,
      "deactivate": true,
      "required": false
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "deployed": {
      "type": "customField",
      "customField": "plugin::thingsboard-plugin.componentLinksComponent"
    },
    "stepStatus": {
      "type": "json"
    }
  }
}
